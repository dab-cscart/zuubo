<?xml version="1.0"?>
<addon scheme="2.0">
    <id>social_buttons</id>
    <version>1.0</version>
    <default_language>en</default_language>
    <name>Social buttons</name>
    <description>Social media and e-mail share buttons (Twitter, Facebook, etc.)</description>
    <priority>100</priority>
    <status>active</status>
    <auto_install>MULTIVENDOR,ULTIMATE</auto_install>
    <translations>
        <item for="name" lang="ru">Социальные кнопки</item>
        <item for="description" lang="ru">Добавляет кнопки популярных соц. сервисов (Twitter, Facebook и т.д.)</item>
    </translations>
    <settings edition_type="ROOT,ULT:VENDOR">
        <sections>
            <section id="twitter">
                <name>Twitter</name>
                <translations>
                    <item lang="ru">Twitter</item>
                </translations>
                <items>
                    <item id="twitter_header">
                        <type>header</type>
                        <name>Twitter settings</name>
                        <translations>
                            <item lang="ru">Настройки Twitter</item>
                        </translations>
                        <variants/>
                    </item>
                    <item id="twitter_enable">
                        <type>checkbox</type>
                        <name>Show Twitter button</name>
                        <translations>
                            <item lang="ru">Показывать кнопку Twitter</item>
                        </translations>
                    </item>
                    <item id="twitter_via">
                        <type>input</type>
                        <name>Via</name>
                        <tooltip>User name to attribute the Tweet to (i.e. 'via @username')</tooltip>
                        <translations>
                            <item lang="ru">Via</item>
                            <item lang="ru" for="tooltip">Имя пользователя, которое будет упомянуто в сообщениях (via @username)</item>
                        </translations>
                    </item>
                    <item id="twitter_size">
                        <type>selectbox</type>
                        <name>Button size</name>
                        <default_value>medium</default_value>
                        <variants>
                            <item id="medium">
                                <name>Medium</name>
                                <translations>
                                    <item lang="ru">Средняя</item>
                                </translations>                                                         
                            </item>
                            <item id="large">
                               <name>Large</name>
                               <translations>
                                   <item lang="ru">Большая</item>
                               </translations>                                                         
                            </item>
                        </variants>
                        <translations>
                            <item lang="ru">Размер кнопки</item>
                        </translations>
                    </item>
                    <item id="twitter_display_count">
                        <type>selectbox</type>
                        <name>Display count</name>
                        <default_value>horizontal</default_value>
                        <variants>
                            <item id="none">
                                <name>None</name>
                                <translations>
                                    <item lang="ru">Не отображать</item>
                                </translations>
                            </item>
                            <item id="horizontal">
                                <name>Horizontal</name>
                                <translations>
                                    <item lang="ru">Горизонтально</item>
                                </translations>
                            </item>
                            <item id="vertical">
                                <name>vertical</name>
                                <translations>
                                    <item lang="ru">Вертикально</item>
                                </translations>
                            </item>
                        </variants>
                        <translations>
                            <item lang="ru">Показывать счетчик</item>
                        </translations>
                    </item>
                    <item id="twitter_display_on">
                        <type>multiple checkboxes</type>
                        <name>Display on</name>
                        <translations>
                            <item lang="ru">Отображать</item>
                        </translations>
                        <variants>
                                <item id="products">
                                    <name>Product details pages</name>
                                    <translations>
                                        <item lang="ru">На страницах товаров</item>
                                    </translations>
                                </item>
                                <item id="pages">
                                    <name>Pages</name>
                                    <translations>
                                        <item lang="ru">На контентных страницах</item>
                                    </translations>
                                </item>
                        </variants>
                    </item>
                </items>
            </section>
            <section id="facebook">
                <name>Facebook</name>
                <translations>
                    <item lang="ru">Facebook</item>
                </translations>
                <items>
                    <item id="facebook_header">
                        <type>header</type>
                        <name>Facebook settings</name>
                        <translations>
                            <item lang="ru">Настройки Facebook</item>
                        </translations>
                    </item>
                    <item id="facebook_enable">
                       <type>checkbox</type>
                       <name>Show Facebook button</name>
                       <translations>
                           <item lang="ru">Показывать кнопку Facebook</item>
                       </translations>
                    </item>
                    <item id="facebook_app_id">
                        <type>input</type>
                        <name>Facebook app ID</name>
                        <tooltip>Specify any Facebook app ID, otherwise the button will not be shown</tooltip>
                        <translations>
                            <item lang="ru">ID приложения Facebook</item>
                            <item lang="ru" for="tooltip">Введите ID любого приложения Facebook, иначе кнопка не будет отображаться</item>
                        </translations>
                    </item>
                    <item id="facebook_href">
                        <type>input</type>
                        <name>The URL of the page to like</name>
                        <tooltip>Leave empty to use an actual page URL</tooltip>
                        <translations>
                            <item lang="ru">URL страницы, которая будет упомянута в сообщении</item>
                            <item lang="ru" for="tooltip">Оставьте пустым, чтобы использовать URL страницы, на которой размещена кнопка</item>
                        </translations>
                    </item>
                    <item id="facebook_send">
                       <type>checkbox</type>
                       <name>Include a Send button with the Like button</name>
                       <default_value>Y</default_value>
                       <translations>
                            <item lang="ru">Отображать кнопку Отправить вместе с кнопкой Мне нравится</item>
                        </translations>
                    </item>
                    <item id="facebook_width">
                        <type>input</type>
                        <name>Width of the Like button (px)</name>
                        <default_value>450</default_value>
                        <translations>
                            <item lang="ru">Ширина кнопки Мне нравится (в пикселях)</item>
                        </translations>
                    </item>
                    <item id="facebook_layout">
                        <type>selectbox</type>
                        <name>Button layout</name>
                        <default_value>button_count</default_value>
                        <variants>
                            <item id="standard">
                                <name>Standard</name>
                                <translations>
                                    <item lang="ru">Стандартный</item>
                                </translations>
                            </item>
                            <item id="button_count">
                                <name>With count</name>
                                <translations>
                                    <item lang="ru">Со счетчиком</item>
                                </translations>
                            </item>
                            <item id="box_count">
                                <name>With large count</name>
                                <translations>
                                    <item lang="ru">С большим счетчиком</item>
                                </translations>
                            </item>
                        </variants>
                        <translations>
                            <item lang="ru">Стиль кнопки</item>
                        </translations>
                    </item>
                    <item id="facebook_show_faces">
                        <type>checkbox</type>
                        <name>Display profile photos</name>
                        <default_value>N</default_value>
                        <tooltip>Enable to display profile photos below the button (standard layout only)</tooltip>
                        <translations>
                            <item lang="ru">Отображать фото пользователей</item>
                            <item lang="ru" for="tooltip">Включите, чтобы отображать фото пользователей под кнопкой (только при стандартном стиле кнопки)</item>
                        </translations>
                    </item>
                    <item id="facebook_action">
                        <type>selectbox</type>
                        <name>Button verb</name>
                        <default_value>like</default_value>
                        <variants>
                            <item id="like">
                                <name>Like</name>
